{
  "name": "js13k-2017",
  "version": "0.0.0",
  "description": "js13k Game Submission Edit",
  "main": "index.js",
  "scripts": {
    "start": "rollup --config rollup.config.dev.js --watch",
    "compile": "rollup --config rollup.config.prod.js",
    "compress": "minify docs -d docs",
    "zip": "bestzip Alter.zip docs",
    "test-size": "stat -c%z Alter.zip",
    "build": "npm run compile && npm run compress && npm run zip && npm run test-size",
    "lint": "eslint src --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HyphnKnight/js13k-2017.git"
  },
  "keywords": [
